# vim: ft=sh

# >>> Language configure >>>
if [[ -z "${LANG}" ]]; then
  export LANG="en_US.UTF-8"
fi
# <<< Language configure <<<

# >>> Paths configure >>>
typeset -gU cdpath fpath mailpath path
# cdpath=(
#   $cdpath
# )

path=(
  ${HOME}/{,s}bin(N)
  /opt/{homebrew,local}/{,s}bin(N)
  /usr/local/{,s}bin(N)
  ${path}
)
# <<< Paths configure <<<
